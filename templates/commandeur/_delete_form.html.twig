<form method="post" action="{{ path('commandeur_delete', {'id': commandeur.id}) }}" onsubmit="return confirm('Etes vous sur de supprimer cet item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ commandeur.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
